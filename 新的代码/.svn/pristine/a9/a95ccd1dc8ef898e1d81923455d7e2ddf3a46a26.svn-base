        $(function () {
            //下拉框选择内容自适应
            function changeOption() {
                var selectLength = $("#Belongs").width();
                $("#Belongs").find("option").each(function () {
                    var titleLength = $(this).attr("title").visualLength("#Belongs");
                    var oneLength = "单".visualLength();//获取单个字符长度
                    if (titleLength > selectLength - 3 * oneLength) {
                        var subLength = Math.floor((selectLength / titleLength) * $(this).attr("title").length);
                        $(this).text($(this).attr("title").substring(0, subLength - 3) + "...");
                    } else {
                        $(this).text($(this).attr("title"));
                    }
                    var subLength = selectLength
                });
            }

            String.prototype.visualLength = function (id) {
                var size = $(id).css("font-size");
                var family = $(id).css("font-family");
                var ruler = $("#ruler").css({
                    "font-size": size || "inherit",
                    "font-family": family || "inherit"
                });
                ruler.text(this);
                return ruler.width();
            }

            $("#Belongs").mouseover(function () {
                changeOption();
            });
        })
    </script>
