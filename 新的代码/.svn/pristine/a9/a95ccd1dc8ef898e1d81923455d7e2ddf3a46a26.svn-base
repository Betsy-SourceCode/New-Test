        function set_Province(CountryId,ProvinceId,CityId) {
            var c = $("#" + CountryId).find("select")[0].value;
            var SelectProvince1 = "";
            $.ajax({
                type: "post",
                async: false,
                dataType: 'JSON',
                url: "/Customer/SelectProvince",
