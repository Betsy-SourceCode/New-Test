    <script>
        function Updates() {
            var CustomerID = @ViewBag.AllCustomer.CustomerID;
            var createBy = '@ViewBag.AllCustomer.CreateBy1';
            var createDept = '@ViewBag.AllCustomer.CreateDept1';
            $.ajax({
                type: "post",
                async: false,
                url: "/Customer/Authority",
                data: {
                    "createBy": createBy,
                    "createDept": createDept
                },
                success: function (result) {
                    if (result == 3 || result==4) {  //显示页签
                        $("#myTab").css("display", "block");
                        $("#Updateform").css("margin-top", "");
                        $("#xianshi").css("display", "block");
                    }
                    else {
                        window.location.href = "/Customer/UpdateCustomer?CustomerID=" + CustomerID + "&num=" + 0;
                        $("#myTab").css("display", "block");
                        $("#Two").css("display", "none");
                        $("#Three").css("display", "none");
                        $("#Four").css("display", "none");

                    }
                }
            })
        }
        function centerModals() {
            $('#themodal').each(function (i) {
                var $clone = $(this).clone().css('display', 'block').appendTo('body');
                var top = Math.round(($clone.height() - $clone.find('.modal-content').height()) / 2);
                top = top > 0 ? top : 0;
                $clone.remove();
                $(this).find('.modal-content').css("margin-top", top);
            });
            $('#themodals').each(function (i) {
                var $clone = $(this).clone().css('display', 'block').appendTo('body');
                var top = Math.round(($clone.height() - $clone.find('.modal-content').height()) / 2);
