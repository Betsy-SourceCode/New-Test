    <script>
        $(function () {
            @*if ("@ViewBag.SP" == "True") {
                $("#Quanxian").css("display", "block");   //有审核权限
            }*@
            $("#myTab a").parent("li").css("border-bottom-color", "transparent");
            $("#myTab a").parent("li").siblings().css("border-bottom", "1px solid #ddd");
            var num = '@ViewBag.num';
            var tabId = "#jichu";
            if (num == "4") {
                Updates();
                $('#myTab a:last').tab('show');
                tabId = "#lianxi";
            }
            if (num == "3") {
                Updates();
                $('#myTab a[href="#xinyong"]').tab('show');
                tabId = "#xinyong";
            }
            if (num == "2") {
                Updates();
                $('#myTab a[href="#zhangdan"]').tab('show');
                tabId = "#zhangdan";
            }
            if (num == "1") {
                Updates();
                $('#myTab a[href="#jichu"]').tab('show');
                tabId = "#jichu";
                $("#xianshi").css("display", "none");
            }
            if (num == "0") {
                $('#myTab a[href="#jichu"]').tab('show');
                tabId = "#jichu";
                $("#xianshi").css("display", "none");
                $("#myTab").css("display", "block");
                $("#Two").css("display", "none");
                $("#Three").css("display", "none");
                $("#Four").css("display", "none");
                $("#Updateform").css("margin-top","");
            }
            var color = $(tabId).css("background-color");
            $("#xianshi").css("background-color", color);
            $("#lasts").css("background-color", color);
            $("#tabContent").css("background-color", color);
            $("#myTab a").click(function (e) {
                $(this).parent("li").css("border-bottom", "none");
                $(this).parent("li").siblings().css("border-bottom", "1px solid #ddd");
                var id = $(this).attr("href");
                var color = $(id).css("background-color");
                $("#xianshi").css("background-color", color);
                $("#lasts").css("background-color", color);
                $("#tabContent").css("background-color", color);

            });
            $("#myTab a").click(function (e) {
                var id = $(this).attr("href");
                var oldId = $("#myTab li.active").find("a").attr("href");
                var CustomerID = '@ViewBag.AllCustomer.CustomerID';
                if (oldId != "#lianxi") {
                    // checkModification(id);
                }
                if (id == "#lianxi") {
                    if ('@ViewBag.AllCustomer.Status' != "审核") {
                        $("#InsertBtn").css("display", "inline");
                    }
                    $("#SaveBtn").css("display", "none");
                    $("#xianshi").css("display", "block");
                }
                else {
                    $("#InsertBtn").css("display", "none");
                    if ('@ViewBag.AllCustomer.Status' != "审核") {   //只能看不能修改
                        $("#SaveBtn").css("display", "inline");
                    }
                    if (id == "#jichu") {
                        $("#xianshi").css("display", "none");
                    }
                    else {
                        $("#xianshi").css("display", "block");
                    }
                }
            });
            //上市公司勾选框赋值
            var IsList = '@ViewBag.AllCustomer.IsList';
            if (IsList == "True") {
                $("#chk_IsList").prop("checked", true);
            }
        })
    </script>
