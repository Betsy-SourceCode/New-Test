    <script>
        //权限等级
        function Updates() {
            var CustomerID = @ViewBag.AllCustomer.CustomerID;
            var createBy = '@ViewBag.AllCustomer.CreateBy1';
            var createDept = '@ViewBag.AllCustomer.CreateDept1';
            $.ajax({
                type: "post",
                async: false,
                url: "/Customer/Authority",
                data: {
                    "createBy": createBy,
                    "createDept": createDept
                },
                success: function (result) {
                    if (result>1||'@ViewBag.SP' == "True") {  //显示页签
                        $("#myTab").css("display", "block");
                        $("#Updateform").css("margin-top", "");
                        $("#xianshi").css("display", "block");
                        if (result > 2) {
                           // $("#Quanxian").css("display", "block");   //有审核权限
                        }
                        $("#XGbtn").css("display", "inline");
                    }
                    else {
                        window.location.href = "/Customer/OnlyReadDetail?CustomerID=" + CustomerID + "&createBy=" + '@ViewBag.username' + "&createDept=" + '@ViewBag.DeptID' + CustomerID + "&num=" + 0;

                    }
                }
            })
        }
        function history() {
        var userid = '@ViewBag.id';
        window.location.href = "/Customer/Default?userid="+userid;
        }
        //修改判断权限
        function Update() {
        var CustomerID = @ViewBag.AllCustomer.CustomerID;
        var createBy = '@ViewBag.AllCustomer.CreateBy1';
        var createDept = '@ViewBag.AllCustomer.CreateDept1';
            $.ajax({
                type: "post",
                async: false,
                url: "/Customer/Authority",
                data: {
                    "createBy": createBy,
                    "createDept": createDept
                },
                success: function (result) {
                    if (result > 2) {
                        window.location.href = "/Customer/UpdateCustomer?CustomerID=" + CustomerID + "&num=" + 1;
                    }
                    else { alert('抱歉,您没有修改本条数据的权限！'); }
                }
            });
        }
        //打印
        function Dayin() { window.print(); };
    </script>
