            var MDCreateTime = new Date().Format("yyyy-MM-dd hh:mm:ss");
            var num = 1;
            if (cwk()) {
                $.ajax({
                    type: "post",
                    async: false,
                    url: "/FunctionOperation/AuditInfoUpdate",
                    data: {
                        "CustomerId": CustomerId,
                        "CWACOpinion": CWACOpinion,
                        "ACCreateBy": ACCreateBy,
                        "ACCreateDept": ACCreateDept,
                        "ACCreateTime": ACCreateTime,
                        "MDCreateTime":MDCreateTime,
                        "num": num
                    },
                    success: function (result) {
                        if (result > 0) {
                            alert("审核成功！");
                            $("#Caiwubu").attr("style", " pointer-events:none;");
                            window.location.href = "/Customer/OnlyReadDetail?CustomerID=" + CustomerId + "&createBy=" + '@ViewBag.createBy' + "&createDept=" + '@ViewBag.createDept' + "&num=" + 1;
                        }
                        else {
                            alert("审核失败，发生错误，请联系电脑部！内部成员请查看日志文件！");
                        }
                    }

                })
            }
            else {
                alert("财务部意见必须填写！");
            }

