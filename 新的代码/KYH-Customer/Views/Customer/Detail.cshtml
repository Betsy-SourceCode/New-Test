    <script>
        $(function () {
            $("#OperationChi").css("display", "");
             //限制审核状态的用户看到修改按钮
            if ('@ViewBag.AllCustomer.Status' == "审核") {
                $("#XGbtn").css("display", "none");
            }
            //权限为3和4可以有提交权限
            var result = Update(1);
            if (result> 2) {
                $("#Quanxian").css("display", "block");
                $("#Caiwubu").attr("style", " pointer-events:none;"); //不可编辑
                    if ('@ViewBag.AllCustomer.Status' == "维护") {
                        $("#Shichangbu").attr("style", " pointer-events:auto;");  //客户状态是维护的话，财务部补充意见关闭，市场部意见打开
                        $("#TiJiao").css("display", "inline");
                }
            }
            else {
                $("#XGbtn").css("display", "none");    //权限低的人看不到修改按钮
            }
            //有审核权限
            if ("@ViewBag.SP" == "True") {
                $("#Quanxian").css("display", "block");
                if ('@ViewBag.AllCustomer.Status' == "审核") {
                    $("#CaiwubuTxt").css("pointer-events", "auto");  //财务部补充意见打开
                    $("#TuiHui").css("display", "inline");
                    $("#TongGuo").css("display", "inline");
                }
            }
            //多选框绑定数据
            if ('@ViewBag.AuditInfo.SORcvEntity' != '' && '@ViewBag.AuditInfo.MFGProcEntity' != '') {
                var SORcvEntity = '@ViewBag.AuditInfo.SORcvEntity'.split(";");
                $.each(SORcvEntity, function (i, item) {
                    $("input[name='SORcvEntity'][value='" + item + "']").attr("checked", "checked");
                });

                var MFGProcEntity = '@ViewBag.AuditInfo.MFGProcEntity'.split(";");
                $.each(MFGProcEntity, function (i, item) {
                    $("input[name='MFGProcEntity'][value='" + item + "']").attr("checked", "checked");
                });

                var DocCheckList = '@ViewBag.AuditInfo.DocCheckList'.split(";");
                $.each(DocCheckList, function (i, item) {
                    $("input[name='DocCheckList'][value='" + item + "']").attr("checked", "checked");
                });
            }
            });
    </script>
