            var FamilyTree = $("#Guanxi").val();
            var SCACOpinion = $("#ShichangbuTxt").val();
            var MDCreateBy ="";
            var MDCreateDept ="";
            var MDCreateTime = new Date().Format("yyyy-MM-dd hh:mm:ss");
            var ACCreateTime = new Date().Format("yyyy-MM-dd hh:mm:ss");
            if (duoxuan()) {
                $.ajax({
