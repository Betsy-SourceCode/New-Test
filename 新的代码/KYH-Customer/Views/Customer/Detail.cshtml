                async: false,
                    url: "/FunctionOperation/AuditInfoAdd",
                data: {
                    "CustomerId": CustomerId,
                    "SORcvEntity": SORcvEntity,   //销售订单接收实体
                    "MFGProcEntity": MFGProcEntity,  //制造生产实体
                    "FamilyTree": FamilyTree,  //关系
                    "DocCheckList": DocCheckList, //文件核对清单
                    "SCACOpinion": SCACOpinion, //市场销售部意见文本框
                    "MDCreateBy": MDCreateBy,
                    "MDCreateDept": MDCreateDept,
                    "MDCreateTime": MDCreateTime,
                    "ACCreateTime": ACCreateTime
