        //修改判断权限
        function Update(num) {
            var CustomerID = @ViewBag.AllCustomer.CustomerID;
            var createBy = '@ViewBag.AllCustomer.CreateBy1';
            var createDept = '@ViewBag.AllCustomer.CreateDept1';
            $.ajax({
                type: "post",
                async: false,
                url: "/Customer/Authority",
                data: {
                    "createBy": createBy,
                    "createDept": createDept
                },
                success: function (result) {
                    if (num == 1) {
                        num= result;
                    }
                    else {
                        if (result > 2) {
                            window.location.href = "/Customer/UpdateCustomer?CustomerID=" + CustomerID + "&num=" + 1;
                        }
                        else {
                            alert('抱歉,您没有修改本条数据的权限！');
                        }
                    }
                }
            });
            return num;
        }
        //多选框验证
        function duoxuan() {
            var flag = false;
            $("input[name='SORcvEntity']:checkbox").each(function () {
                if ($(this).prop('checked')) {
                    flag = true;
                }
            })
            if (flag) {
                var flag1 = false;
                $("input[name='MFGProcEntity']:checkbox").each(function () {
                    if ($(this).prop('checked')) {
                        flag1 = true;
                    }
                })
                if (flag1) {
                    return true;
                } else {
                    alert('制造生产实体,至少选择一项');
                    document.getElementById("CheckBox5").focus();
                    return false;
                }
            } else {
                alert('销售订单接收实体,至少选择一项');
                document.getElementById("CheckBox4").focus();
                return false;
            }

        }
        Date.prototype.Format = function (fmt) { // author: meizz
            var o = {
                "M+": this.getMonth() + 1, // 月份
                "d+": this.getDate(), // 日
                "h+": this.getHours(), // 小时
                "m+": this.getMinutes(), // 分
                "s+": this.getSeconds(), // 秒
                "q+": Math.floor((this.getMonth() + 3) / 3), // 季度
                "S": this.getMilliseconds() // 毫秒
            };
            if (/(y+)/.test(fmt))
                fmt = fmt.replace(RegExp.$1, (this.getFullYear() + "").substr(4 - RegExp.$1.length));
            for (var k in o)
                if (new RegExp("(" + k + ")").test(fmt)) fmt = fmt.replace(RegExp.$1, (RegExp.$1.length == 1) ? (o[k]) : (("00" + o[k]).substr(("" + o[k]).length)));
            return fmt;
        }
        //打印
        function Dayin() { window.print();};
